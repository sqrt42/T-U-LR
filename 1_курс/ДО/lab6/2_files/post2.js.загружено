	else
	  {
	  var btnForm=document.getElementById('saveform');
	  if(btnForm){btnForm.insertAdjacentHTML('afterend','<button rel="popover" type="button" class="btn btn-lg btn-danger margin10" data-container="body" data-toggle="popover" data-placement="top" data-content="Чтобы скачать решение в Word, отключите, пожалуйста, блокираторы типа AdBlock и перезагрузите страницу" onclick="clkPopover(this);">Скачать Word</button>');destroyParent(btnForm);}
