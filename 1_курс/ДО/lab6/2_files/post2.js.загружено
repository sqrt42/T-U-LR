	  }
	}
	});
    }
  if(IsAdBlock()==0)
    {
    if(das=='0'){adm1.innerHTML='';adm1.className='';if(btnWord){btnWord.insertAdjacentHTML('afterend','<button rel="popover" type="button" class="btn btn-lg btn-danger" data-container="body" data-toggle="popover" data-placement="top" data-content="Чтобы скачать решение в Word, отключите, пожалуйста, блокираторы типа AdBlock и перезагрузите страницу" onclick="clkPopover(this);">Скачать Word</button>');destroyParent(btnWord);}}
