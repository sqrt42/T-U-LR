function MathQuillFormul()
{
var sq=addscript("https://math.semestr.ru/skin/mathquill/mathquill.js",0);
sq.onload=function()
  {
  var MQ = MathQuill.getInterface(2);
  var sel=document.getElementsByClassName('mathquill'),kol=sel.length;
  for (var i=0; i<kol; i++)
    {
    var problemSpan=sel[i];
    MQ.StaticMath(problemSpan);
    }
  }
}
function LoadWord(event)
{
process.innerHTML='Загрузка файла ...';
processtime.innerHTML='';
if(process.innerHTML!='')
  {
  var prctimerId=setInterval(function(){processtime.innerHTML+='|&#9632;|';var m=processtime.innerHTML.split('||');if(m.length>=15){var prctimerId=Number(processtime.getAttribute('data-tid'));processtime.innerHTML='';process.innerHTML='';clearInterval(prctimerId);CloseModal('myProcess');}},200);
  processtime.setAttribute('data-tid',prctimerId);
  }
ShowModal('myProcess');
return 1;
}
function CtreateBtn()
{
if(sf && document.getElementById('savebtn')==null){var btn=document.createElement('input');btn.type='submit';btn.id='savebtn';btn.name='crc';btn.className='btn btn-lg btn-success';btn.value='Скачать в Word';sf.action='/fileload.php';sf.appendChild(btn);if(document.getElementById('savebtn1')){var card=document.getElementById('savebtn1');card.parentNode.removeNode(card);}}
var savebtn=document.getElementById('savebtn');
if(sfdoc && document.getElementById('savebtndoc')==null){var btn=document.createElement('input');btn.type='submit';btn.id='savebtndoc';btn.name='crc';btn.className='btn btn-lg btn-success';btn.value='Формат docx';sfdoc.action='/fileload.php';sfdoc.appendChild(btn);if(document.getElementById('savebtn1')){var card=document.getElementById('savebtn1');card.parentNode.removeNode(card);}}
var sbdoc=document.getElementById('savebtndoc');
if(sfe && document.getElementById('savexcel')==null){var btn=document.createElement('input');btn.type='submit';btn.id='savexcel';btn.name='excel';btn.className='btn btn-lg btn-success';btn.value='Скачать Excel';sfe.action='/fileload.php';sfe.appendChild(btn);}
var sbe=document.getElementById('savexcel');
if(sf && savebtn){sf.action='/fileload.php';savebtn.disabled=false;}
if(sfdoc && sbdoc){sfdoc.action='/fileload.php';sbdoc.disabled=false;}
if(sfe && sbe){sfe.action='/fileload.php';sbe.disabled=false;}
}
function LoadBtn()
{
if(sf)sf.addEventListener("submit",LoadWord,false);
if(sfdoc)sfdoc.addEventListener("submit",LoadWord,false);
}
$(document).on( "click", "#saveAll", function(event)
    {
    document.getElementById('saveAll').disabled=true;
    var fd=new FormData();
